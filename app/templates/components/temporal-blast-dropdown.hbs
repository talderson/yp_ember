<BsDropdown as |dd|>
  <dd.button>
    {{#if (is-equal @blastType "1")}}
      Production
    {{else if (is-equal @blastType "2")}}
      Development
    {{else}}
      All
    {{/if}}
    <span class="caret"></span>
  </dd.button>
  <dd.menu as |ddm|>
      <ddm.item>
        {{#link-to 'temporal-graphs.temporal-graph' instrumentId modelName (query-params blast_type=null) class="dropdown-item"}}All{{/link-to}}
      </ddm.item>
      <ddm.item>
        {{#link-to 'temporal-graphs.temporal-graph' instrumentId modelName (query-params blast_type=1) class="dropdown-item"}}Production{{/link-to}}
      </ddm.item>
      <ddm.item>
        {{#link-to 'temporal-graphs.temporal-graph' instrumentId modelName (query-params blast_type=2) class="dropdown-item"}}Development{{/link-to}}
      </ddm.item>
  </dd.menu>
</BsDropdown>