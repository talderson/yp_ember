<BsDropdown as |dd|>
  <dd.button>
    {{@frequencySelect}}
    <span class="caret"></span>
  </dd.button>
  <dd.menu as |ddm|>
      <ddm.item>
        {{#link-to 'spatial-graphs.spatial-graph' instrumentId modelName (query-params frequency_num=0 frequency='daily' events_only=false) class="dropdown-item"}}Daily{{/link-to}}
      </ddm.item>
      <ddm.item>
        {{#link-to 'spatial-graphs.spatial-graph' instrumentId modelName (query-params frequency_num=0 frequency='weekly' events_only=false) class="dropdown-item"}}Weekly{{/link-to}}
      </ddm.item>
      <ddm.item>
        {{#link-to 'spatial-graphs.spatial-graph' instrumentId modelName (query-params frequency_num=0 frequency='monthly' events_only=false) class="dropdown-item"}}Monthly{{/link-to}}
      </ddm.item>
  </dd.menu>
</BsDropdown>